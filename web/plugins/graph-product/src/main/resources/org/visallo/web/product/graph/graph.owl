<?xml version="1.0"?>
<rdf:RDF xmlns="http://visallo.org/workspace/product/graph#"
     xml:base="http://visallo.org/workspace/product/graph"
     xmlns:visallo="http://visallo.org#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://visallo.org/product">
        <owl:imports rdf:resource="http://visallo.org/workspace"/>
        <owl:imports rdf:resource="http://visallo.org"/>
    </owl:Ontology>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <!-- http://visallo.org/workspace/product/graph#toNode -->

    <owl:ObjectProperty rdf:about="http://visallo.org/workspace/product/graph#toNode">
        <rdfs:domain rdf:resource="http://visallo.org/workspace#product"/>
        <rdfs:domain rdf:resource="http://visallo.org/workspace/product/graph#compoundNode"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label xml:lang="en">To Node</rdfs:label>
    </owl:ObjectProperty>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- http://visallo.org/workspace/product/graph#entityPosition -->

    <owl:DatatypeProperty rdf:about="http://visallo.org/workspace/product/graph#entityPosition">
        <visallo:objectPropertyDomain rdf:resource="http://visallo.org/workspace/product#toEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <visallo:textIndexHints>NONE</visallo:textIndexHints>
        <visallo:userVisible>false</visallo:userVisible>
    </owl:DatatypeProperty>




    <!-- http://visallo.org/workspace/product/graph#parentNode -->

    <owl:DatatypeProperty rdf:about="http://visallo.org/workspace/product/graph#parentNode">
        <visallo:objectPropertyDomain rdf:resource="http://visallo.org/workspace/product#toEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <visallo:textIndexHints>NONE</visallo:textIndexHints>
        <visallo:userVisible>false</visallo:userVisible>
    </owl:DatatypeProperty>




    <!-- http://visallo.org/workspace/product/graph#nodeChildren -->

    <owl:DatatypeProperty rdf:about="http://visallo.org/workspace/product/graph#nodeChildren">
        <visallo:objectPropertyDomain rdf:resource="http://visallo.org/workspace/product#toEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <visallo:textIndexHints>NONE</visallo:textIndexHints>
        <visallo:userVisible>false</visallo:userVisible>
    </owl:DatatypeProperty>




    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <!-- http://visallo.org/workspace/product/graph#compoundNode -->

    <owl:Class rdf:about="http://visallo.org/workspace/product/graph#compoundNode">
        <visallo:userVisible>false</visallo:userVisible>
        <rdfs:label xml:lang="en">Graph Product Compound Node</rdfs:label>
    </owl:Class>

</rdf:RDF>
